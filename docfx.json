{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ],
          "src": "./"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml", "api/index.md" ],
        "src": "./",
        "dest": "./"
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ],
        "src": "./",
        "dest": "./"
      }
    ],
    "overwrite": [
      {
        "files": [ "apidoc/**.md" ],
        "src": "./",
        "dest": "./"
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}